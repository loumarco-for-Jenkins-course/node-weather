// Pipeline lab end of chapter 3
pipeline {
    agent any
    stages {
        stage('pull-from-github-build') {
            steps {
                bat "npm install"
            }
			post {
				failure {
					echo "I failed (sob)"
				}
			}
        }
    }
    post { 
        always { 
            echo 'Pipeline executed after change!'
        }
    }
}